package com.zozofood.builder;

import com.zozofood.dto.RestaurantCreationResponseDto;
import com.zozofood.model.Restaurant;

public class RestaurantDtoResponseBuilder {
	
	public final RestaurantCreationResponseDto buildRestaurantCreaionRespDtoFromRestaurant(Restaurant restaurant) {
		return RestaurantCreationResponseDto.builder()
				.restaurantId(restaurant.getRestaurantId())
				.restaurantName(restaurant.getRestaurantName())
				.build();
	}
	

}
